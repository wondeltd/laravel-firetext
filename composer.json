{
    "name": "wondeltd/laravel-firetext",
    "description": "A Laravel wrapper for the firetext/php-sdk package",
    "type": "library",
    "require": {
		"firetext/php-sdk": "^1.0",
		"illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
	},
	"keywords": [
		"laravel",
		"firetext",
		"wonde"
	],
    "license": "MIT",
    "authors": [
        {
			"name": "Ross Cooper",
			"email": "ross@razorcreations.com"
        }
	],
	"autoload": {
		"psr-4": {
			"Wondeltd\\FireText\\": "src/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"Wondeltd\\FireText\\ServiceProvider"
			]
		}
	}
}
